while wait() do
    if getgenv().Door == true then
        for i,v in pairs(game.Workspace.Doors:GetChildren()) do
            for i,l in pairs(v:GetChildren()) do
                if l:FindFirstChild("???") then
                    l.BrickColor = BrickColor.new("Lime green")
                end
            end
        end
    end
end
end)

